@model List<MvcApplication1.Models.myclass>
@{Layout = null;}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <link href="~/Content/styles.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.js"></script>

    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript">

        $(document).ready(function () {
            $("#search").on("click", function () {
                alert("hello");
                var userName = $('#n1').val();
                $.ajax({
                    type: 'GET',
                    data: {
                        id: userName
                    },
                    url: '@Url.Action("search", "Student")',
                    success: function (item) {
                        window.alert("AGAYA");
                        $.each(item, function (i, data) {
                            $('#t1').append('<tr><td>' + data.name + ' <td>&nbsp;</td></td><td>' + data.age_ + ' <td>&nbsp;</td></td><td>' + data.last_degree + ' <td>&nbsp;</td></td><td>' + data.locations + ' <td>&nbsp;</td></td></tr>' + '<a href=/Apointment/fixAp/?id= ' + data.dues_in_range + '>Apointment</a>');

                        });
                    },
                    error: function (d) {
                        window.alert("Error Occured Please Try Later");
                    }
                });
            });
        });
    </script>

</head>
<body style="margin-left:100px;margin-right:100px">
    <nav class="navbar navbar-inverse row" role="navigation">
        <div class="container">
            <div>
                <a class="navbar-brand" style="padding-top:25px;color:blue;font-weight:bold" href="/Student/hview">TIME ACADEMY.net.pk</a>
            </div>
            <div class="navbar-header">
                <button type="button" value="TIME ACADEMY.net" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li style="margin-left:40px;" class="navbar-brand"><a href="/Student/viewTeacher">view teacherList</a></li>
                    <li style="margin-left:40px;" class="navbar-brand"><a href="/Student/update">update</a></li>
                   
                    <li style="margin-left:40px;" class="navbar-brand"><a href="/Student/delete">delete yourself</a></li>
                   
                </ul>
            </div>
        </div>
    </nav>
   
    <div class="input-group">
        <input type="text" class="form-control" id="n1" name="n1" placeholder="Search teacher">
        <input type="button" value="Search" id="search" class="btn btn-warning" />

    </div>


    <div align="center">
        <table cellpadding="3" border="1" id="t1">
            <tr>
                <td><span class="label label-warning">Teacher name </span></td>
                <td>&nbsp;</td>
                <td><span class="label label-warning">age</span></td>
                <td>&nbsp;</td>
                <td><span class="label label-warning">last degree </span></td>
                <td>&nbsp;</td>
                <td><span class="label label-warning">loaction</span></td>
                <td>&nbsp;</td>
               
            </tr>
        </table>
    </div>

    
</body>
</html>

