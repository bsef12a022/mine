@model List<MvcApplication1.Models.student>
@{Layout = null;}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <link href="~/Content/styles.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <link rel="stylesheet" href="/resources/demos/style.css">
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
   
</head>
<body style="margin-left:100px;margin-right:100px">
    <nav class="navbar navbar-inverse row" role="navigation">
        <div class="container">
            <div>
                <a class="navbar-brand" style="padding-top:25px;color:blue;font-weight:bold" href="/Admin/viewStudent">TIME ACADEMY.net.pk</a>
            </div>
            <div class="navbar-header">
                <button type="button" value="TIME ACADEMY.net" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li style="margin-left:40px;" class="navbar-brand"><a href="Admin/hview">Back</a></li>
                </ul>
            </div>
        </div>
    </nav>

    <h2>StudentList</h2>
    






    <table border="1" cellpadding="15" cellspacing="10" bordercolor="00FF00">
        <tr>
            <td class="col-lg-3">Name </td>
            <td class="col-lg-3">age </td>
            <td class="col-lg-3">institute name </td>
            <td class="col-lg-3">subjects </td>

            <td class="col-lg-3">address</td>
            <td class="col-lg-3">contacts_num </td>
       



        </tr>


        @for (int i = 0; i < Model.Count  ; i++)
        {
            <tr>
               
                <td class="col-lg-3">@Model[i].name</td>
                <td name="user" class="col-lg-3">@Model[i].age</td>
                <td class="col-lg-3">@Model[i].institute_name</td>
                <td class="col-lg-3">@Model[i].subjects</td>
                <td class="col-lg-3">@Model[i].address</td>

                <td class="col-lg-3">@Model[i].contact_num </td>
           
                @*@Html.ActionLink("send", "send?id="+@Model[i].user + "& dt="+@dat , "Admin")*@


                @*<td class="col-lg-3">  <input type="text" id="datepicker" onclick="f1()"> </td>*@
            </tr>
        }
    </table>

</body>
</html>
